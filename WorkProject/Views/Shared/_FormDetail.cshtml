@model List<EntityLayer.Concrete.Form>
@using EntityLayer.Concrete
<h1>FORM LİSTELERİ</h1>
<table id="tbl1" class="table table-bordered">
    <thead>
        <tr>
        <th>ID</th>
        <th>Form Name</th>
        <th>DESCRIPTION</th>
        <th>CREATED AT</th>
        <th>CREATED BY</th>
        <th>NAME</th>
        <th>SURNAME</th>
        <th>AGE</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.FormName</td>
                        <td>@item.Description</td>
                        <td>@item.CreatedAt</td>
                        <td>@item.CreatedBy</td>
                        <td>@item.Name</td>
                        <td>@item.Surname</td>
                        <td>@item.Age</td>
                    </tr>
        }
    </tbody>
</table>
